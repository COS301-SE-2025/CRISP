    "ogl": "^1.0.11",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.28.0",
    "three": "^0.180.0"
